/* pages/index/failure/index.wxss */

.yh-pre-mast {
  box-shadow: 0px 0px 12rpx #f1f1f1 ;
  margin: 0px 28rpx;
  border-radius: 20rpx;
  box-sizing: border-box;
  padding: 0rpx 28rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  /* border:2rpx solid #f1f1f1; */
}
.yh-pre-mall {
  margin-top: 28rpx;
  padding-bottom: 28rpx;
}
.yh-pre-num {
  color: #4d4d4d;
  box-sizing: border-box;
  padding: 28rpx 56rpx;
  padding-top: 35rpx;
  width: 100%;
}

.yh-pre-nus {
  padding-left: 0px;
}
.yh-evalu-tex {
  border: 1px solid #49a0ff;
  border-radius: 10px;
  box-sizing: border-box;
  padding: 25rpx 25rpx;
  height: 304rpx;
  margin-top: 30rpx;
}

.yh-evalu-tex text {
  color: #ccc;
  font-size: 22rpx;
  float: right;
}

.yh-evalu-tex textarea {
  height: 230rpx;
}

.place-holder {
  color: #d4d4de;
  font-size: 24rpx;
  line-height: 30rpx;
}
.yh-pre-btn{
  width: 299rpx !important;
  height: 80rpx;
  line-height: 54rpx;
  color: #fff;
  text-align: center;
  margin: 120rpx auto;
  background-color: #49A0FF !important;
  border-radius: 40rpx;
}