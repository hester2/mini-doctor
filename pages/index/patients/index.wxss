.yh-shade {
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  top: 121rpx;
  z-index: 9;
  background: rgba(255, 255, 255, 1);
}

.yh-shade-black {
  background: rgba(0, 0, 0, 0.5);
}

.yh-filter {
  width: 100%;
  background: #fff;
  position: fixed;
  height: 50px;
  z-index: 9;
  left: 0;
  top: 120rpx;
  border-bottom: 1px solid #f2f3f4;
  box-sizing: border-box;
  padding: 20rpx 0px;
  display: flex;
  flex-wrap: nowrap;
}

.yh-filter .items {
  display: flex;
  flex-wrap: nowrap;
  align-items: center;
  width: 34%;
  justify-content: center;
  height: 32px;
  line-height: 32px;
  border-right: 2rpx solid #eee;
}

.yh-mo {
  width: 120rpx;
  height: 32px;
  line-height: 32px;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.yh-filter .items image {
  display: block;
  width: 28rpx;
  height: 16rpx;
  margin-left: 10rpx;
}

.yh-filter .items text {
  margin-left: 10rpx;
  font-size: 24rpx;
}

.yh-filter-options {
  width: 100%;
  position: absolute;
  overflow: hidden;
  overflow-y: auto;
  z-index: 10;
  padding: 2px 0;
  right: 0;
  top: 50px;
  background: #fff;
  max-height: 572rpx !important;
}

.yh-filter-options .option {
  display: flex;
  justify-content: space-between;
  border-bottom: 1px solid #f8f8f8;
  text-indent: 2em;
  height: 100rpx;
  line-height: 100rpx;
}

.yh-filter-options .option text {
  margin-right: 30rpx;
  font-size: 30rpx;
  color: #f00;
  font-weight: 700;
}

.yh-filter-options .option:last-child {
  border: none;
}

.yh-filter-options .current {
  color: #f00;
  font-weight: 700;
}

.yh-filter-buttons {
  display: flex;
  width: 100%;
  flex-wrap: nowrap;
  position: absolute;
  z-index: 11;
  left: 0;
  bottom: 0;
  height: 100rpx;
  background: #fff;
}

.yh-filter-buttons view {
  width: 50%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  position: relative;
}

.yh-filter-buttons view:last-child {
  background: #49a0ff;
  color: #fff;
}

.yh-filter-buttons view button {
  opacity: 0;
  width: 100%;
  position: absolute;
  z-index: 9;
  left: 0;
  top: 0;
  height: 100rpx;
}

.yh-mast-title {
  height: 120rpx;
  position: fixed;
  background-color: #49a0ff !important;
  display: flex;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  padding: 0px 25rpx;
  top: 0px;
  width: 100%;
}

.yh-mast-search {
  width: 100%;
  height: 75rpx;
  background-color: #fff !important;
  display: flex;
  border-radius: 55rpx;
  justify-content: flex-end;
  background-repeat: no-repeat;
  background-size: 36rpx 36rpx;
  background-position-x: 305rpx;
  background-position-y: center;
}

.yh-mast-search .input {
  height: 75rpx;
  width: 90%;
  border-radius: 55rpx;
  text-align: center;
  font-size: 30rpx;
  background-color: transparent;
  color: #000 !important;
}

.yh-mast-sear {
  width: 100%;
  display: flex;
  align-items: center;
  color: #fff;
}

.yh-mast-canc {
  height: 75rpx;
  line-height: 75rpx;
  color: #fff;
  margin-left: 30rpx;
}

.yh-mast-searchs {
  width: 86%;
  background-position-x: 30rpx;
  background-repeat: no-repeat;
}

.yh-mast-searchs .input {
  text-align: left !important;
  box-sizing: border-box;
}

.grace-body {
  display: block;
  background-color: #fff;
  width: 100%;
  height: 100%;
  position: fixed;
  z-index: 999999;
  top: 160rpx;
  box-sizing: border-box;
}

.yh-mast-fl {
  display: flex;
  flex-wrap: wrap;
  margin-bottom: 60rpx;
  margin-top: 10rpx;
}

.yh-mast-fo {
